{
  "name": "slipstream-gasless-relayer",
  "version": "1.0.0",
  "description": "TypeScript EVM Relayer for Slipstream gasless transfers on Kadena, Base and Arbitrum testnets with Pyth integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:integration": "ts-node test-integration.ts",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ethereum",
    "kadena",
    "base",
    "arbitrum",
    "gasless",
    "relayer",
    "evm",
    "pyusd",
    "usdc",
    "slipstream",
    "defi",
    "typescript",
    "pyth"
  ],
  "author": "Slipstream Team",
  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.9",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^9.0.6",
    "axios": "^1.6.2",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
